<%-include("partials/header")-%>
<h1>Compose</h1>
<form class="" action="/compose" method="post">
  <div class="form-group">
    <label for="composeTitle">Title</label>
    <input type="text" class="form-control" name="composeTitle">
  </div>
  <div class="form-group">
    <label for="composeText">Compose</label>
    <textarea type="text" class="form-control"  name="composeText" rows="4"></textarea>
  </div>
  <button type="submit" class="btn btn-primary" name="button">Publish</button>
</form>
<%- include("partials/footer")-%>
